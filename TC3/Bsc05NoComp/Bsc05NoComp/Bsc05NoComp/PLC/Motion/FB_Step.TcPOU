<?xml version="1.0" encoding="utf-8"?>
<TcPlcObject Version="1.1.0.1">
  <POU Name="FB_Step" Id="{0aea8e66-d7a8-409b-9f89-3625b52daf60}" SpecialFunc="None">
    <Declaration><![CDATA[FUNCTION_BLOCK FB_Step
VAR_INPUT
	bEnable : BOOL;
	fTime : LREAL;
	fSignal : LREAL;
	fTimeConst : LREAL;
	fDivider : LREAL;
END_VAR
VAR_OUTPUT
	fOutput : LREAL;
END_VAR
VAR
END_VAR
]]></Declaration>
    <Implementation>
      <ST><![CDATA[
IF bEnable AND fTime < fTimeConst THEN
	fOutput := fSignal / fDivider;
ELSIF bEnable AND fTime < 2 * fTimeConst THEN
	fOutput := fSignal;
ELSIF bEnable AND fTime < 3 * fTimeConst THEN
	fOutput := 0.0;
END_IF]]></ST>
    </Implementation>
  </POU>
</TcPlcObject>