  "actionTemplates": [
    {
      "displayname": "ToogleBool",
      "name": "ToogleBool",
      "description": "The Action template toggles a boolean value",
      "category": "Action Templates",
      "actions": [
        {
          "objectType": "Condition",
          "active": true,
          "parts": [
            {
              "if": [
                {
                  "compare1": {
                    "objectType": "StaticValue",
                    "valueType": "tchmi:general#/definitions/String",
                    "value": "BoolToToggle"
                  },
                  "compare2": {
                    "objectType": "StaticValue",
                    "valueType": "tchmi:general#/definitions/String",
                    "value": "False"
                  },
                  "compareOperator": "==",
                  "logic": null
                }
              ],
              "then": [
                {
                  "objectType": "WriteToSymbol",
                  "active": true,
                  "symbolExpression": "BoolToToggle",
                  "value": {
                    "objectType": "StaticValue",
                    "valueType": "tchmi:general#/definitions/String",
                    "value": "True"
                  },
                  "asyncWait": true
                }
              ]
            },
            {
              "else": [
                {
                  "objectType": "WriteToSymbol",
                  "active": true,
                  "symbolExpression": "BoolToToggle",
                  "value": {
                    "objectType": "StaticValue",
                    "valueType": "tchmi:general#/definitions/String",
                    "value": "False"
                  },
                  "asyncWait": true
                }
              ]
            }
          ],
          "asyncWait": true
        }
      ],
      "parameters": [
        {
          "name": "BoolToToggle",
          "type": "tchmi:general#/definitions/Boolean",
          "description": "This is the input parameter"
        }
      ]
    },
    {
      "displayname": "lys",
      "name": "lys",
      "category": "Action Templates",
      "actions": [
        {
          "objectType": "Condition",
          "active": true,
          "parts": [
            {
              "if": [
                {
                  "compare1": {
                    "objectType": "StaticValue",
                    "valueType": "tchmi:general#/definitions/String",
                    "value": "BoolToToggle"
                  },
                  "compare2": {
                    "objectType": "StaticValue",
                    "valueType": "tchmi:general#/definitions/String",
                    "value": "True"
                  },
                  "compareOperator": "==",
                  "logic": null
                }
              ],
              "then": [
                {
                  "objectType": "WriteToSymbol",
                  "active": true,
                  "symbolExpression": "%ctrl%TcHmiEllipse_1::BackgroundColor%/ctrl%",
                  "value": {
                    "objectType": "StaticValue",
                    "valueType": "tchmi:framework#/definitions/Color",
                    "value": {
                      "color": "rgba(0, 255, 24, 1)"
                    }
                  },
                  "asyncWait": true
                }
              ]
            },
            {
              "else": [
                {
                  "objectType": "WriteToSymbol",
                  "active": true,
                  "symbolExpression": "%ctrl%TcHmiEllipse_1::BackgroundColor%/ctrl%",
                  "value": {
                    "objectType": "StaticValue",
                    "valueType": "tchmi:framework#/definitions/Color",
                    "value": {
                      "color": "rgba(255, 0, 0, 1)"
                    }
                  },
                  "asyncWait": true
                }
              ]
            }
          ],
          "asyncWait": true
        }
      ],
      "parameters": [
        {
          "name": "Lyser",
          "type": "tchmi:general#/definitions/Boolean",
          "description": "Lyset skal blinke grønt når BoolToToggle == True"
        }
      ]
    }
  ],
